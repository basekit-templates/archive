
{% extends "layout.twig" %}

{% block content %}

  <header class="bk-header">
      <div class="companyheader wrapper">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
          	{{widget('extendednavigation', 'navigationheader', {})|raw}}
       </div>
  </header> 

<div class="canvas wrapper">
		{{zone('main')|raw}}
</div>

<footer class="bk-footer">
	<div class="footer-border companyheader">
		{{widget('profile', 'socialicons', { 'profileType' : 'socialicons' })|raw}}
	</div>
</footer>

{% endblock %}
